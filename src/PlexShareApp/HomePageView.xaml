<Window x:Class="PlexShareApp.HomePageView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlexShareApp"
        mc:Ignorable="d"
        WindowStartupLocation = "CenterScreen"
        Title="HomePageView" Height="850" Width="1500">
    <Grid Background="Red">
        <Frame x:Name="HomePage" NavigationUIVisibility="Hidden" />
        <StackPanel>
            <Button Click="toMainScreen">Click to Main Screen</Button>
        </StackPanel>
    </Grid>
</Window>
